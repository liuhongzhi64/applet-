/*遮罩层*/

.modal-mask {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 999;
}

/*遮罩内容*/

.modal-content {
  display: flex;
  flex-direction: column;
  width: 90%;
  background-color: #fff;
  border-radius: 20rpx;
  padding: 20rpx;
  text-align: center;
  position: relative;
}

/*中间内容*/

.main-content {
  flex: 1;
  height: 100%;
  overflow-y: hidden;
  max-height: 80vh; /* 内容高度最高80vh 以免内容太多溢出*/
}

/*底部按钮*/

.modal-footer {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 120rpx;
  line-height: 80rpx;
  width: 90%;
  /* border-top: 2rpx solid #d2d3d5; */
  /* margin-top: 30rpx;
  margin-bottom: 20rpx; */
  position: absolute;
  bottom: 30rpx;
  left: 33rpx;
}

.cancel-btn, .confirm-btn {
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  margin-top: 20rpx;
}

.cancel-btn {
  color: #4E89FF;
  background-color: #fff;
  border: 2rpx solid #3a6cfa;
}

.confirm-btn {
  color: #fff;
  background-color: #4E89FF;
  margin-left: 20rpx;
}
